{
    "name": "alexvsezanyato/social",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "require": {
        "symfony/routing": "^7.3",
        "symfony/http-foundation": "^7.3",
        "doctrine/migrations": "^3.9",
        "php-di/php-di": "^7.1",
        "twig/twig": "^3.22",
        "doctrine/dbal": "^4.3",
        "doctrine/orm": "^3.5",
        "symfony/mime": "^7.3",
        "monolog/monolog": "^3.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "scripts": {
        "migrate": "./vendor/bin/doctrine-migrations"
    },
    "require-dev": {
        "pestphp/pest": "^4.1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
